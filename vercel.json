{
  "functions": {
    "api/telegram.js": {
      "memory": 1024,
      "maxDuration": 90
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}
